@{
    var partials = new[] { "_Overview", /* application info, */ "_Bio" };
}
<div class="container">


    <h2>APPLICATION FOR WARRIOR</h2>
    @using (Html.BeginForm())
    {
        for (int i = 0; i < partials.Length; i++)
        {
            var partialView = partials[i];

            @Html.Partial(partialView)
            <div class="row visible-print print-footer">
                <div class="col-xs-3"></div>
                <div class="col-xs-6"></div>
                <div class="col-xs-2">Initial Here_________</div>
                <div class="col-xs-1">Page | @i</div>
            </div>
        }
    }
</div>
